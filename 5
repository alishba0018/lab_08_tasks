#include <stdio.h>

int main() {
    int i,n;
    printf("enter a number:");
    scanf("%d",&n);
    printf("prime numbers till %d are:",n);
    for (int num = 2; num <= n; num++) {
        int isPrime = 1;
        for (i = 2; i * i <= num; i++) {
            if (num % i == 0) {
                isPrime = 0;
                break;
            }
        }

        if (isPrime) {
            printf("%d ", num);
        }
    }
     printf("\n");
    return 0;
}
