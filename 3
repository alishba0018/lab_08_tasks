#include <stdio.h>

int main() {
 int a[2][3][3]={
 {
     1,2,3,
     4,5,6,
     7,8,9
 }, //page 1
  {
     9,8,7,
     6,5,4,
     3,2,1
 } //page 2
 };
 int sum[2]={0,0};
 for(int i=0;i<2;i++){
    for(int j=0;j<3;j++){
       for(int k=0;k<3;k++){
           sum[i]=sum[i]+a[i][j][k];
       }
    }
 }
    for (int b = 0; b < 2; b++) {
        printf("Sum of elements on page %d: %d\n", b + 1, sum[b]);
    }
   
    return 0;
}
